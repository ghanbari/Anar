{% extends '::base.html.twig' %}

{% block body -%}
    <h1>User</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fname</th>
                <td>{{ entity.fname }}</td>
            </tr>
            <tr>
                <th>Lname</th>
                <td>{{ entity.lname }}</td>
            </tr>
            <tr>
                <th>Faname</th>
                <td>{{ entity.faname }}</td>
            </tr>
            <tr>
                <th>Staffcode</th>
                <td>{{ entity.staffCode }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('anar_super_panel_user') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('anar_super_panel_user_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
